{
  "name": "roam-samepage",
  "version": "2.11.5",
  "description": "Official Roam client into the intra tool-for-thought protocol",
  "main": "./build/main.js",
  "scripts": {
    "prebuild:roam": "npm install",
    "build:roam": "samepage build --dry",
    "postinstall": "patch-package --patch-dir node_modules/samepage/patches",
    "start": "samepage dev",
    "test": "samepage test"
  },
  "author": {
    "name": "SamePage",
    "email": "support@samepage.network",
    "url": "https://samepage.network"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/ws": "^8.5.4"
  },
  "dependencies": {
    "roamjs-components": "^0.78.11",
    "samepage": "^0.69.9"
  },
  "samepage": {
    "external": [
      "react-dom/client",
      "@blueprintjs/core=window.Blueprint.Core",
      "@blueprintjs/datetime=window.Blueprint.DateTime",
      "@blueprintjs/select=window.Blueprint.Select",
      "chrono-node=window.ChronoNode",
      "crypto-js=window.CryptoJS",
      "cytoscape=window.RoamLazy.Cytoscape",
      "file-saver=window.FileSaver",
      "jszip=window.RoamLazy.JSZip",
      "idb=window.idb",
      "insect=window.RoamLazy.Insect",
      "marked=window.RoamLazy.Marked",
      "marked-react=window.RoamLazy.MarkedReact",
      "nanoid=window.Nanoid",
      "react=window.React",
      "react-dom=window.ReactDOM",
      "react-youtube=window.ReactYoutube",
      "tslib=window.TSLib"
    ],
    "include": [
      "package.json",
      "README.md"
    ],
    "out": "extension",
    "mirror": ".",
    "format": "esm",
    "install": {
      "steps": [
        {
          "title": "Open Roam Depot",
          "children": "image"
        },
        {
          "title": "Search SamePage",
          "children": "image"
        },
        {
          "title": "Install!",
          "children": "image"
        }
      ]
    }
  }
}
